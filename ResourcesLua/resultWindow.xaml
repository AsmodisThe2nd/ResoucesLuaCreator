<Window x:Class="ResourcesLua.resultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResourcesLua"
        mc:Ignorable="d"
        Title="resultWindow" Height="550" Width="450" MinHeight="550" MinWidth="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="87"/>
            <RowDefinition Height="424*"/>
        </Grid.RowDefinitions>
        <Label x:Name="label" Margin="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
            <TextBlock TextWrapping="WrapWithOverflow" Margin="10">
                Copy the text from below into a new file under "steamcmd\gm_server\garrysmod\lua\autorun\server". The filename does not matter. You can use "workshopResources.lua" for example.
            </TextBlock>
        </Label>
        <TextBox x:Name="textBox" Margin="10" TextWrapping="Wrap" Text="TextBox" Grid.Row="1"/>


    </Grid>
</Window>
